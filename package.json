{
  "name": "openai-codex-electron",
  "productName": "Codex",
  "version": "260205.1301",
  "description": "Codex",
  "main": ".vite/build/main.js",
  "scripts": {
    "compile": "tsc -b",
    "tsc": "tsc -noEmit",
    "dev": "pnpm --dir .. run generate:third-party-notices && NODE_ENV=development electron-forge start",
    "build": "rm -rf out && PNPM_YES=true pnpm run forge:make -- --platform=darwin --arch=arm64",
    "forge:package": "electron-forge package",
    "forge:make": "electron-forge make",
    "forge:publish": "electron-forge publish",
    "format": "prettier --check .",
    "format:fix": "prettier --log-level warn --write .",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "test": "node ./scripts/ensure-electron-binary.mjs && vitest run",
    "test:quiet": "node ./scripts/ensure-electron-binary.mjs && vitest run --silent --reporter=dot",
    "devtools:reset": "sh -c 'rm -rf \"$HOME/Library/Application Support/Codex/extensions/fmkadmapgofadopljbjfkapdkoienihi\" \"$HOME/Library/Application Support/Codex/Service Worker\" \"$HOME/Library/Application Support/Codex/Code Cache\"'",
    "build:sparkle-native": "node ./scripts/build-sparkle-native.mjs",
    "postinstall": "pnpm run rebuild:sqlite",
    "rebuild:sqlite": "node ./scripts/rebuild-sqlite.mjs"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron-forge/shared-types": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/notarize": "^3.1.1",
    "@sentry/cli": "^3.1.0",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/lodash": "^4.17.20",
    "@types/memoizee": "^0.4.12",
    "@types/mime-types": "^3.0.1",
    "debug": "^4.4.1",
    "electron": "40.0.0",
    "electron-devtools-installer": "^4.0.0",
    "electron-installer-dmg": "^5.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "node-addon-api": "^8.5.0",
    "node-gyp": "^10.2.0",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@sentry/electron": "^7.5.0",
    "@sentry/node": "10.29.0",
    "@tanstack/react-form": "^1.27.7",
    "app-server-types": "workspace:*",
    "better-sqlite3": "^12.4.6",
    "electron-context-menu": "^4.1.1",
    "electron-liquid-glass": "1.1.1",
    "electron-squirrel-startup": "^1.0.1",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "mime-types": "^2.1.35",
    "node-pty": "^1.1.0",
    "protocol": "workspace:*",
    "shared-node": "workspace:*",
    "shell-env": "^4.0.1",
    "shlex": "^3.0.0",
    "smol-toml": "^1.5.2",
    "zod": "^4.1.13"
  },
  "codexBuildFlavor": "prod",
  "codexBuildNumber": "554",
  "codexSparkleFeedUrl": "https://persistent.oaistatic.com/codex-app-prod/appcast.xml"
